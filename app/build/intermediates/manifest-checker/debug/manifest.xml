<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kenjin.hotelapp"
    android:versionCode="1"
    android:versionName="1.3" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="25" />

    <application
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.kenjin.hotelapp.activity.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.kenjin.hotelapp.activity.PreviewActivity" />
        <activity android:name="com.kenjin.hotelapp.activity.BookActivity" />
        <activity android:name="com.kenjin.hotelapp.activity.HistoryActivity" />
        <activity android:name="com.kenjin.hotelapp.activity.SurveyActivity" />
        <activity android:name="com.kenjin.hotelapp.activity.ResultSurveyActivity" />
        <activity android:name="com.kenjin.hotelapp.activity.DetailHistoryActivity" />

        <meta-data
            android:name="android.support.VERSION"
            android:value="25.4.0" />

        <provider
            android:name="com.android.tools.ir.server.InstantRunContentProvider"
            android:authorities="com.kenjin.hotelapp.com.android.tools.ir.server.InstantRunContentProvider"
            android:multiprocess="true" />
    </application>

</manifest>